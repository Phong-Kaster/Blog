<div class="container-fluid bg-faded fh5co_padd_mediya padding_786">
    <div class="container padding_786">
        <nav class="navbar navbar-toggleable-md navbar-light ">
            <button class="navbar-toggler navbar-toggler-right mt-3" type="button" data-toggle="collapse"
                    data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                    aria-label="Toggle navigation"><span class="fa fa-bars"></span></button>
            <a class="navbar-brand" href="#"><img src="/images/logo.png" alt="img" class="mobile_logo_width"/></a>
            <% function getChild(myMenus,parent)
            {
                let children = [];
                for( let index = 0 ; index < myMenus.length;index++)
                {
                    if(myMenus[index].parent.toString() == parent.toString())
                    {
                        children.push(myMenus[index]);
                    }
                }
                return children;
            } %>
            <% myMenus.forEach(function(iMenu){ %>  
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <% let children = getChild(myMenus,iMenu._id) ; 
                    if(iMenu.parent == "" && children.length == 0){%>
                        <li class="nav-item">
                            <a class="nav-link" href="<%= iMenu.href  %>" id="home"><%= iMenu.name  %><span class="sr-only">(current)</span></a>
                        </li>
                        <%}else if(iMenu.parent == "" && children.length != 0){%>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="dropdownMenuButton1" data-toggle="dropdown"
                            aria-haspopup="true" aria-expanded="false"><%= iMenu.name  %><span class="sr-only">(current)</span></a>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink_1">
                                    <% children.forEach(function(subMenu){ %>
                                    <a class="dropdown-item" href="<%= subMenu.href %>"> <%= subMenu.name %></a>
                                    <%}) %>
                                </div>
                        </li>
                    <%}%>
                </ul>
            </div>
            <% }) %>
        </nav>
    </div>
</div>
 
